{
  "name": "style-analyzer-pro",
  "description": "Professional design audit tool for websites",
  "private": true,
  "version": "4.3.9",
  "type": "module",
  "scripts": {
    "dev:sqs": "wxt --mode sqs",
    "dev:generic": "wxt --mode generic",
    "build:sqs": "wxt build --mode sqs",
    "build:generic": "wxt build --mode generic",
    "zip:sqs": "wxt zip --mode sqs",
    "zip:generic": "wxt zip --mode generic",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "test:logic": "vitest run",
    "test:e2e": "playwright test",
    "test:ui": "./run-tests-ui.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/chrome": "^0.1.36",
    "@vitest/ui": "^4.0.17",
    "playwright": "^1.57.0",
    "rollup": "^4.55.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "wxt": "^0.18.5"
  },
  "dependencies": {
    "axe-core": "^4.11.1"
  }
}
